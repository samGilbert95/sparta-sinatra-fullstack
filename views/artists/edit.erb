<h1>EDIT PAGE</h1>
<form action="/artist/<%= @artist.id %>" method="POST">
  <div class="form-group">
    <label for="exampleInputEmail1">First Name</label>
    <input type="text" class="form-control" placeholder="Track" name="first_name"
    value = "<%= @artist.first_name %>">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Last Name</label>
    <input type="text" class="form-control" placeholder="Artist" name="last_name"
    value = "<%= @artist.last_name %>">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Year of Birth</label>
    <input type="text" class="form-control" placeholder="Name" name="dob"
    value = "<%= @artist.dob %>">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
  <% if (@artist.id != '') %>
    <input type="hidden" name="_method" value="PUT">
  <% end %>
</form>
